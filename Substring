using System;

namespace _3._Substring
{
    class Program
    {
        static void Main(string[] args)
        {
            string textToRemuve = Console.ReadLine();
            string text = Console.ReadLine();
            
            while (text.Contains(textToRemuve))
            {
                text = text.Replace(textToRemuve, String.Empty);
            }
            Console.WriteLine(text);
        }
    }
}
